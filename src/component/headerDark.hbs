<header class="header">
    {{#with header}}

    <div class="header_top">

        <div class="header_logo">
            <span>{{brandName}}</span>
        </div>

        <nav>

            <div class="nav nav_inline">
                {{#each links}}
                <a href="{{url}}" class="nav_link">{{text}}</a>
                {{/each}}
            </div>

        </nav>
    </div>


    <div class="header_bottom">
        <div class="ad_block">

            <span class="ad_logo">Ad</span>

            {{#with ad}}
            <a class="ad_url" href="{{url}}">{{text}}: {{url}} </a>
            {{/with}}
        </div>
        <div>
            <span class="ad_useronline">Users online: {{userOnline}}</span>
        </div>
    </div>
    {{/with}}
</header>